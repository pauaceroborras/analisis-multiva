dades <- read.csv2("airline_passenger_satisfaction.csv", sep = ",")
dades <- dades[,-1]

View(dades) ### Veure la base de dades
class(dades) ### Classe
dim(dades) ### DimensiÃ³
nrow(dades) ### Nombre de files
ncol(dades) ### Nombre de columnes
colnames(dades) ### Nom de les columnes
rownames(dades) ### Nom de les files
str(dades) ### ConÃ¨ixer lâ€™estructura de la base de dades

#Fem un resum
summary(dades)
